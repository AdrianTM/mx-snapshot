UI gfxboot gfx-cpio syslinux.msg
timeout 600

default live

MENU TITLE Welcome to MX-15 (Fusion)

LABEL live
    MENU LABEL custom-name
    KERNEL /antiX/vmlinuz
    APPEND quiet 
    INITRD /antiX/initrd.gz

LABEL harddisk
    MENU LABEL Boot_from_Hard_Disk
    COM32 chain.c32
    APPEND hd0

LABEL memtest
    MENU LABEL Memory_Test
    KERNEL /boot/memtest
